<h1>lista</h1>

<% @todos.each do |todo| %>

<p><%="#{todo.description} "%></p>

<%= link_to 'Ver', todo%>
<%= link_to 'Editar',edit_path(todo)%>
 <%= link_to 'Eliminar', todo, method: :delete , data:{confirm:'seguro que quieres borrar!!'}%>
 <%= link_to 'completado',completed_path(todo)%>
<%end%>
